<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.box">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserInfoActivity" />
        <activity android:name=".EnclosureActivity" />
        <activity android:name=".LeavingAlarmActivity" />
        <activity android:name=".SoundActivity" />
        <activity android:name=".SystemSettingActivity" />
        <activity android:name=".EditUserActivity" />
        <activity android:name=".LoginOrRegisterActivity" />
        <activity android:name=".BoxAddActivity" />
        <activity android:name=".BoxActivity" />
        <activity android:name=".AddUserActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".AddEnclosureActivity" />
        <activity android:name=".PersonalInfoActivity" />
        <activity android:name=".PersonalSettingActivity" />

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="com.example.cameraalbumtest.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_path" />
        </provider>

        <activity android:name=".SettingQxActivity"></activity>
    </application>

</manifest>